<package>
	<id>com.jp.macosdynwp</id>
	<name>macOS Dynamic Wallpapers</name>
	<version>1.2</version>
	<compatibility>
		<firmware>
			<miniOS>13.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>wallpaperloader</package>
	</dependencies>
	<shortDescription>Appearance aware wallpapers from macOS Mojave and Catalina for iOS 13. Uses WallpaperLoader by Skitty. Get it from this repo: https://skitty.xyz/repo/</shortDescription>
	<descriptionlist>
		<description>Appearance aware wallpapers from macOS Mojave and Catalina that change with iOS 13 dark mode.</description>
		<description>Uses WallpaperLoader by Skitty.</description>
		<description>Repo: https://skitty.xyz/repo/</description>
	</descriptionlist>
